<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"><!--  mention version 5.0 or 3.0 -->

    <bean id="s" class="com.bhavitha.SpringDemo.StudentClass">
    	<property name="name" value="Bhavitha GB"></property>
    	<property name="marks" ref="m"></property>
    </bean>
    
    <bean id="m" class="com.bhavitha.SpringDemo.Marks">
    	<property name="phy" value="95"></property>
    	<property name="chem" value="90"></property>
    	<property name="math" value="100"></property>
    </bean>

</beans>

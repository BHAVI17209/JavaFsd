<?xml version="1.0" encoding="UTF-8"?>
<web-app>
  <display-name>SecondProject</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>registr</servlet-name>
	<servlet-class>com.bhavitha.demo.RegisterServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>registr</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>logn</servlet-name>
  	<servlet-class>com.bhavitha.demo.Generic</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>logn</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  

</web-app>